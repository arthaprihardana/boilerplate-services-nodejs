{
  "name": "boilerplate-services-nodejs",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arthaprihardana/boilerplate-services-nodejs.git"
  },
  "description": "Core Server Backend",
  "main": "/bin/www",
  "scripts": {
    "start": "BABEL_ENV=development nodemon --exec 'babel-node' bin/www",
    "build": "BABEL_ENV=staging gulp build",
    "build:min": "BABEL_ENV=production gulp build && uglifyjs dist/app.js -o dist/app.min.js -c -m",
    "clean": "gulp clean && gulp cleanMinified",
    "staging": "node bin/www",
    "production": "node bin/www"
  },
  "keywords": [
    "Core Server Backend",
    "REST API NodeJS"
  ],
  "author": {
    "name": "Artha Prihardana",
    "email": "arthaprihardana@gmail.com"
  },
  "license": "ISC",
